{
  "extends": "airbnb",
  "plugins": ["react", "jsx-a11y", "import"],
  "rules": {
    "react/jsx-filename-extension": "off",
    "jsx-a11y/img-has-alt": "off",
    "jsx-a11y/href-no-hash": "off",
    "import/no-extraneous-dependencies": "off",
    "react/no-array-index-key": "off"
  },
  "env": {
    "jest": true
  }
}
